{
    "version": 2,
    "builds": [
        {
            "src": "src/app.ts",
            "use": "@vercel/node"
        },
        {
            "src": "uploads/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "handle": "filesystem",
            "src": "uploads/(.*)",
            "destination": "uploads/$1"
        },
        {
            "src": "/(.*)",
            "dest": "src/app.ts"
        }
    ]
}